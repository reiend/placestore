{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "composite": true,
    "moduleResolution": "node",
    "lib": [
      "dom",
      "esnext"
    ],
    "jsx": "react-jsx",
    "rootDir": "src",
    "strict": true,
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "vite.config.ts"
  ]
}
