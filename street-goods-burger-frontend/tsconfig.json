{
  "compilerOptions": {
    "target": "es5",
    "jsx": "preserve",
    "module": "ESNext",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "paths": {
      "@components/*": ["./src/res/components/*"]
    }
  }
}
